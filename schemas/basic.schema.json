{
    "title": "basic test",
    "description": "json validate test",
    "type": "object",
    "required": ["name", "age", "sex"],
    "properties": {
        "name": {
            "type": "string",
            "minLength": 3,
            "maxLength": 10
        },
        "age": {
            "type": "integer",
            "minimum": 16,
            "maximum": 100
        },
        "sex": {
            "type": "string",
            "pattern": "^male|female$"
        }
    }
}